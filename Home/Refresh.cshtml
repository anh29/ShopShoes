@model ShopLearning.Models.ThongKeModel
@if (Model != null)
            {
                <div class="thongke">
                    <table class="table">
                        <tr>
                            <td>Hôm nay</td>
                            <td>@Model.HomNay</td>
                        </tr>
                        <tr>
                            <td>Hôm Qua</td>
                            <td>@Model.HomQua</td>
                        </tr>
                        <tr>
                            <td>Tuần Này</td>
                            <td>@Model.TuanNay</td>
                        </tr>
                        <tr>
                            <td>Số người online</td>
                            <td>@ViewBag.Visitors_online</td>
                        </tr>
                    </table>
                </div>
            }
